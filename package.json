{
  "name": "next-utils",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snek152/next-utils.git"
  },
  "scripts": {
    "build": "pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "start": "watch -p 'src/**/*.ts' -c 'pnpm build'",
    "prepare": "husky install"
  },
  "keywords": [
    "typescript",
    "nextjs"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "author": "SneK152",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "next": "^12.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "4.7.4",
    "watch-cli": "^0.2.3",
    "husky": "^8.0.0"
  },
  "peerDependencies": {
    "next": "^12.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
